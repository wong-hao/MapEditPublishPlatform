            <AnnoFontSize>3</AnnoFontSize>
            <AnnoFontName>黑体</AnnoFontName>
            <AnnoNote>地级市政府驻地</AnnoNote>
            <AnnoColor>K100</AnnoColor>
          </DataRow>
          <DataRow unit="毫米" display="true">
            <AnnoName>蜀山区</AnnoName>
            <AnnoFontSize>2.5</AnnoFontSize>
            <AnnoFontName>黑体</AnnoFontName>
            <AnnoNote>县（区）政府驻地</AnnoNote>
            <AnnoColor>K100</AnnoColor>
          </DataRow>
          <DataRow unit="毫米" display="true">
            <AnnoName>稻香村街道</AnnoName>
            <AnnoFontSize>2</AnnoFontSize>
            <AnnoFontName>黑体</AnnoFontName>
            <AnnoNote>乡、镇街办驻地</AnnoNote>
            <AnnoColor>K100</AnnoColor>
          </DataRow>
          <DataRow unit="毫米" display="true">
            <AnnoName>青阳路社区</AnnoName>
            <AnnoFontSize>1.8</AnnoFontSize>
            <AnnoFontName>黑体</AnnoFontName>
            <AnnoNote>村庄</AnnoNote>
            <AnnoColor>K100</AnnoColor>
          </DataRow>
        </Layer>
        <Layer LayerName="自然、文化保护区界" Checked="true" RuleID="2" />
